 prices = { "banana": 4, "apple": 2, "orange": 1.5, "pear": 3 }

def most_expensive(prices):
	highest_price_key = None
	for key in prices:
		if highest_price_key == None:
			highest_price_key = key
		elif prices[key] > prices[highest_price_key]:
			highest_price 
